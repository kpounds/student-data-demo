{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "operator-linebreak": "off",
    "react/jsx-filename-extension": "off",
    "function-paren-newline": 0,
    "arrow-parens": ["error", "as-needed"],
    "fix": true
  }
}
